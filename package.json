{
  "name": "reactbootstrapbp",
  "version": "1.0.0",
  "description": "Boilerplate for using React Bootstrap and webpack",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server",
    "build": "npm run clean && NODE_ENV=production webpack -p",
    "watch": "npm run clean && NODE_ENV=production webpack --watch",
    "clean": "rimraf ./dist/*",
    "test": "jest --coverage --color --verbose"
  },
  "author": "leojavier",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap-loader": "^2.2.0",
    "bootstrap-sass": "^3.3.7",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "import-loader": "^1.0.1",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.8",
    "precss": "^2.0.0",
    "resolve-url-loader": "^2.1.1",
    "sass-loader": "^6.0.6",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.7.1"
